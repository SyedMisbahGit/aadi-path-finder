{
  "expo": {
    "name": "Al-Naseeh V3",
    "slug": "al-naseeh-mobile",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alnaseeh.mobile",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app uses the microphone for voice input and AI conversations.",
        "NSCameraUsageDescription": "This app uses the camera to scan documents and upload images.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to upload documents and images.",
        "NSLocationWhenInUseUsageDescription": "This app uses location to provide region-specific college recommendations.",
        "NSFaceIDUsageDescription": "This app uses Face ID for secure authentication.",
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2563eb"
      },
      "package": "com.alnaseeh.mobile",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "alnaseeh"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2563eb",
          "sounds": ["./assets/notification-sound.wav"]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Al-Naseeh to access your camera to scan documents and take photos."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Al-Naseeh to access your photo library to upload documents and images."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Al-Naseeh to access your microphone for voice input and AI conversations."
        }
      ]
    ],
    "scheme": "alnaseeh",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-expo-project-id"
      }
    },
    "owner": "your-expo-username",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/your-expo-project-id"
    }
  }
} 